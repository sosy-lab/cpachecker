{
  "name": "counterexample",
  "version": "1.0.0",
  "description": "Report generated by CPAchecker",
  "author": "",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "node scripts/startBuild.js && webpack",
    "start": "npm run build & webpack serve --mode=development",
    "test": "node scripts/startBuild.js && karma start karma.conf.js",
    "e2e-test": "protractor e2e-conf.js",
    "lint:fix": "node scripts/startBuild.js && eslint --fix --ignore-path .lintignores $(pwd)'/**/*.js' && prettier --ignore-path .lintignores --write $(pwd)'/**/*.+(css|json|html)'",
    "lint": "node scripts/startBuild.js && eslint --max-warnings 0 --ignore-path .lintignores . && prettier --ignore-path .lintignores --check $(pwd)'/**/*.+(css|json|html)'"
  },
  "devDependencies": {
    "@webpack-cli/serve": "^1.6.1",
    "angular-mocks": "^1.8.2",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "is-docker": "^2.2.1",
    "jasmine": "^3.99.0",
    "jasmine-jquery": "^2.1.1",
    "karma": "^6.3.11",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "^4.0.1",
    "karma-jquery": "^0.2.4",
    "karma-webpack": "^5.0.0",
    "license-checker": "^25.0.1",
    "mini-css-extract-plugin": "^1.6.2",
    "prettier": "^2.5.1",
    "protractor": "^7.0.0",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "terser-webpack-plugin": "^5.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "angular": "^1.8.2",
    "angular-sanitize": "^1.8.2",
    "bootstrap": "^4.6.1",
    "buffer": "^6.0.3",
    "code-prettify": "^0.1.0",
    "datatables.net": "^1.11.4",
    "datatables.net-dt": "^1.11.4",
    "jquery": "^3.6.0",
    "jquery-ui-dist": "^1.13.0",
    "pako": "^2.0.4",
    "popper.js": "^1.16.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "protractor": true,
      "jasmine": true,
      "jest": true
    },
    "globals": {
      "angular": "readonly",
      "argJson": "readonly",
      "argTabDisabled": "readonly",
      "cfaJson": "readonly",
      "cfaSplit": "readonly",
      "devData": "readonly",
      "d3": "readonly",
      "dagreD3": "readonly",
      "dependencies": "readonly",
      "functions": "readonly",
      "functionCallEdges": "readonly",
      "graphSplitThreshold": "readonly",
      "init": "readonly",
      "margin": "readonly",
      "PR": "readonly",
      "render": "readonly",
      "spyOnEvent": "readonly",
      "workerData": "readonly",
      "zoomEnabled": "readonly"
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "jest"
    ],
    "rules": {
      "global-require": 0,
      "prettier/prettier": "error",
      "no-param-reassign": 0,
      "new-cap": 0,
      "no-alert": 0,
      "prefer-destructuring": 0,
      "no-console": 0,
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "**/*conf.js",
            "webpack.config.js",
            "scripts/*.js"
          ]
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    }
  }
}
