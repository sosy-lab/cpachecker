<project name="SymbolicExpressionParserGeneration" default="generate-parser" basedir="./../../../../../../..">
    <!-- basedir should be the "CPAchecker" directory -->

    <property name="dir.parser" location="src/org/sosy_lab/cpachecker/cpa/constraints/contraint/" />
    <property name="scanner.source" value="SymbolicExpressionScanner.jflex"/>
    <property name="parser.source" value="SymbolicExpressionParser.cup"/>
    <property name="scanner.target" value="SymbolicExpressionScanner"/>
    <property name="parser.target" value="SymbolicExpressionParser"/>
    <property name="symbols.target" value="SymExprSymbols"/>

    <import file="${basedir}/build/build-parser.xml"/>
</project>
