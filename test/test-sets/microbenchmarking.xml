<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.0//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.0.dtd">
<benchmark tool="cpachecker" timelimit="900 s" hardtimelimit="1000 s" memlimit="15 GB" cpuCores="4">
  <option name="-noout"></option> 
  <resultfiles>output/*.txt</resultfiles>
  
  <tasks name="ReachSafety-Loops">
  	<requiredfiles>../programs/benchmarks/openssl/s3_srvr.blast.03.i.cil.c</requiredfiles>
    <includesfile>microbenchmarking.set</includesfile>
    <propertyfile>../programs/benchmarks/properties/unreach-call.prp</propertyfile>
  </tasks>
  
  <rundefinition>
    <option name="-config">config/microbenchmarking.properties</option>
    <option name="-setprop">microBenchmark.outputFile=output.txt</option>
    <option name="-setprop">microBenchmark.programFiles=test/programs/benchmarks/openssl/s3_srvr.blast.03.i.cil.c,test/programs/benchmarks/openssl/s3_srvr.blast.03.i.cil.c</option>
  </rundefinition>
</benchmark>
