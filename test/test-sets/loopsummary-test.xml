    <option name="-benchmark"/>
  </rundefinition>
  
  <rundefinition name="predicate-value">
    <option name="-config">config/loopsummary/predicateAnalysis-valueAnalysis.properties</option>
    <option name="-benchmark"/>
  </rundefinition>
 
  <rundefinition name="sv-comp22-summaries">
    <option name="-svcomp22--summaries"/>
    <option name="-benchmark"/>
  </rundefinition>

  <rundefinition name="sv-comp22">
    <option name="-svcomp22"/>
    <option name="-benchmark"/>
  </rundefinition>  

  <rundefinition name="loopsummary-predicate">
	  <option name="-config">config/loopsummary/predicateAnalysis-loopsummary.properties</option>
	  <option name="-benchmark"/>
  </rundefinition>

  <rundefinition name="predicate">
	  <option name="-config">config/predicateAnalysis.properties</option>
	  <option name="-benchmark"/>
  </rundefinition>

  <rundefinition name="loopsummary-value">
	  <option name="-config">config/loopsummary/valueAnalysis-loopsummary.properties</option>
	  <option name="-benchmark"/>
  </rundefinition>

  <rundefinition name="value">
	  <option name="-config">config/valueAnalysis.properties</option>
 	  <option name="-benchmark"/>
  </rundefinition>
  
  <rundefinition name="loopsummary-bmc">
    <option name="-config">config/loopsummary/bmc-incremental-loopsummary.properties</option>
    <option name="-benchmark"/>
  </rundefinition>

  <rundefinition name="bmc">
    <option name="-config">config/loopsummary/bmc-incremental.properties</option>
    <option name="-benchmark"/>
  </rundefinition>
  
  

  <resultfiles>**.graphml*</resultfiles>

