<benchmark tool="cpachecker" timelimit="900 s" hardtimelimit="960 s" memlimit="15 GB" cpuCores="4">
  <option name="-heap">10000M</option>
    
  <rundefinition name="value-loopsummary-gar">
    <option name="-config">config/loopsummary/valueAnalysis-loopsummary-gar.properties</option>
    <option name="-benchmark"/>
  </rundefinition>
  
  <rundefinition name="predicate-loopsummary-gar">
    <option name="-config">config/loopsummary/predicateAnalysis-loopsummary-gar.properties</option>
    <option name="-benchmark"/>
