<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.9//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.9.dtd">
<benchmark tool="cpachecker" timelimit="900 s" hardtimelimit="960 s" memlimit="15 GB" cpuCores="4">
  <option name="-heap">10000M</option>

  <rundefinition name="loopsummary-value">
          <option name="-config">config/loop-summary/valueAnalysis-loopsummary--overflow.properties</option>
  </rundefinition>

  <rundefinition name="loopsummary-predicate">
          <option name="-config">config/loop-summary/predicateAnalysis-loopsummary--overflow.properties</option>
  </rundefinition>

  <rundefinition name="predicate">
          <option name="-config">config/predicateAnalysis--overflow.properties</option>
          <option name="-setprop">counterexample.export.enabled=false</option>
  </rundefinition>

  <rundefinition name="value">
          <option name="-config">config/valueAnalysis--overflow.properties</option>
          <option name="-setprop">counterexample.export.enabled=false</option>
  </rundefinition>

  <rundefinition name="sv-comp21">
    <option name="-svcomp21"/>
    <option name="-benchmark"/>
  </rundefinition>

  <resultfiles>**.graphml*</resultfiles>

  <tasks name="NoOverflows-BitVectors">
    <includesfile>../programs/benchmarks/NoOverflows-BitVectors.set</includesfile>
    <propertyfile>../programs/benchmarks/properties/no-overflow.prp</propertyfile>
  </tasks>
  <tasks name="NoOverflows-Other">
    <includesfile>../programs/benchmarks/NoOverflows-Other.set</includesfile>
    <propertyfile>../programs/benchmarks/properties/no-overflow.prp</propertyfile>
  </tasks>

  <tasks name="SoftwareSystems-BusyBox-NoOverflows">
