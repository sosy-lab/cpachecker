<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.0//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.0.dtd">
<benchmark tool="cpachecker" timelimit="900 s" hardtimelimit="960 s" memlimit="15 GB" cpuCores="8">

  <option name="-benchmark"/>
  <option name="-noout"/>

  <option name="-heap">10000M</option>
  <option name="-timelimit">900s</option>

  <rundefinition>
    <option name="-smg2"/>
	<option name="-smg2-CEGAR"/>
	<option name="-ValueAnalysis"/>
  </rundefinition>

  <tasks name="ReachSafety-Heap">
    <includesfile>../programs/benchmarks/ReachSafety-Heap.set</includesfile>
    <propertyfile>../programs/benchmarks/properties/unreach-call.prp</propertyfile>
  </tasks>

</benchmark>
