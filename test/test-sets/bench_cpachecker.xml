<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.9//EN"
        "https://www.sosy-lab.org/benchexec/benchmark-1.9.dtd">
<benchmark tool="cpachecker" timelimit="900 s" hardtimelimit="960 s" memlimit="15 GB" cpuCores="2">
    <resultfiles>output/**.*</resultfiles>
    <!--
    <rundefinition name="cpachecker-predicate">
        <option name="-config">config/predicateAnalysis.properties</option>
        <option name="-benchmark"/>
</rundefinition>

	<rundefinition name="cpachecker-svcomp22">
		<option name="-config">config/svcomp22.properties</option>
        <option name="-benchmark"/>
    </rundefinition>
-->
    <rundefinition name="cpachecker-loopsummary-predicate">
        <option name="-config">config/loopsummary/predicateAnalysis-loopsummary.properties</option>
        <option name="-setprop">cfa.summaries.strategies=OUTPUTLOOPACCELERATION,LOOPCONSTANTEXTRAPOLATION,NAIVELOOPACCELERATION,HAVOCSTRATEGY</option>

        <option name="-benchmark"/>
    </rundefinition>

    <tasks name="ReachSafety-Loops">
        <includesfile>../programs/benchmarks/ReachSafety-Loops.set</includesfile>
        <propertyfile>../programs/benchmarks/properties/unreach-call.prp</propertyfile>
    </tasks>
</benchmark>
