<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 2.3//EN" "https://www.sosy-lab.org/benchexec/benchmark-2.3.dtd">
<benchmark tool="cpachecker" timelimit="900 s" hardtimelimit="960 s" memlimit="15 GB" cpuCores="4">

    <option name="--no-output-files"/>
    <option name="--heap">2000M</option>

    <rundefinition name="SV-COMP25_valid-memsafety">
        <tasks name="C.valid-memsafety">
            <includesfile>../../../sv-benchmarks/c/MemSafety-Arrays.set</includesfile>
            <includesfile>../../../sv-benchmarks/c/MemSafety-Heap.set</includesfile>
            <includesfile>../../../sv-benchmarks/c/MemSafety-Juliet.set</includesfile>
            <includesfile>../../../sv-benchmarks/c/MemSafety-LinkedLists.set</includesfile>
            <includesfile>../../../sv-benchmarks/c/MemSafety-MemCleanup.set</includesfile>
            <includesfile>../../../sv-benchmarks/c/MemSafety-Other.set</includesfile>
            <propertyfile>../../config/properties/valid-memsafety.prp</propertyfile>
        </tasks>
    </rundefinition>
    <columns>
        <column title="UsedConfig">Used algorithm property</column>
    </columns>
</benchmark>
