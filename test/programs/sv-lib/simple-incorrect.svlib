(set-logic LIA)

(set-info :format-version 1.0)

(declare-var w Int)
(declare-var z Int)
(define-proc f1 ((x Int) (y Int)) () () (!(sequence (assume (= x y)) (! (assume (= x y)) :check-true (not (= x y)))) :tag proc-f1))
(verify-call f1 (w z))