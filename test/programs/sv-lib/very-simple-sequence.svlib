(set-logic LIA)

(set-info :format-version 1.0)

(define-proc f1 ((x Int) (y Int)) () () (sequence (assume (= x y)) (assume (= x y)) (assume (= x y))))

(define-proc f2 ((x Int) (y Int)) () () (sequence (assume (= x y)) (assume (= x y))))